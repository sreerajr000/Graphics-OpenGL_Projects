#pragma once

#include "block.h"

std::vector<std::vector<int> > frame (18, std::vector<int>(10, 0));

for(int i = 0; i < frame.size(); i++){
	for(int j = 0; j < frame[i].size(); j++)
		cout << frame[i][j] << " ";
	cout << "\n";
}
